<%= form_for @profile do |f| %>

<p>
  <%= f.label :full_name %>
  <%= f.text_field :full_name %>
</p>

<p>
  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>
</p>

<p>
  <%= f.label :birth_date %>
  <%= f.text_field :birth_date %>
</p>

<p>
  <%= f.label :education %>
  <%= f.text_field :education %>
</p>

<p>
  <%= f.label :about_me %>
  <%= f.text_field :about_me %>
</p>

<p>
  <%=f.submit %>
</p>

<% end %>
