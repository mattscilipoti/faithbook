<%= form_for [@profile, @comment] do |f| %>

  <p>
    <%= f.label :full_name %>
    <%= f.text_field :full_name %>
  </p>

  <p>
    <%= f.label :photo_url %>
    <%= f.text_field :photo_url %>
  </p>

  <p>
    <%= f.label :date_time %>
    <%= f.text_field :date_time %>
  </p>

  <p>
    <%= f.label :content %>
    <%= f.text_field :content %>
  </p>

  <p>
    <%=f.submit %>
  </p>

<% end %>
